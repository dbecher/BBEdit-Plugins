    <key>Language Features</key>
    <dict>
        <key>Comment Pattern</key>
        <string><![CDATA[(?x:
          (?>                                       (?# Jade-style comment using //)
            ^(?P<leadspace>^\s*)                    (?# Capture leading whitespace before comment delimiter)
            \/\/.*$                                 (?# First line of comment)
            (?m:                                    (?# Capture subsequent lines that are indented by the same)
              \r(?P=leadspace)[ \t]+(?:.+)          (?#   ammount as the first line, then at least one space or)
            )*                                      (?#   tab, then any amount of text)
          ) |
          (?>	\<\!--		(?s:.*?)	(?: --\> | \z )	) (?# HTML-style comment)
        )]]></string>
        <key>Function Pattern</key>
        <string><![CDATA[(?x:                       (?# Map blocks to functions)
          (?P<leadspaces>^\s*)                      (?# Capture leading indentation)
          (?P<function>                             (?# Blocks are defined by: block <string>)
            ((?:block(?:[ ]+ (?:append) | (?:prepend))?) | (?:append) | (?:prepend)) [ \t]+
            (?P<function_name>                      (?# Function name is the name of the block)
              [a-zA-Z0-9_-]+
            )
            [ \t]*$                                 (?# optional spaces or tabs at end of line)
            (?m:                                    (?# Capture subsequent lines that are indented by the same)
              \r(?P=leadspaces)[ \t]+(?:.+)         (?#   ammount as the first line, then at least one space or)
            )+                                      (?#   tab, then any text)
          )
        )]]></string>
        <key>Identifier and Keyword Character Class</key>
        <string><![CDATA[a-zA-Z0-9-_!]]></string>
        <key>String Pattern</key>
        <string><![CDATA[(?x:
            (".+?")    |
            ('.+?')    |
            (\#\{.+?\})
        )]]></string>
        <key>Skip Pattern</key>
        <string><![CDATA[(?x:
            (?P>comment) |
            (?P>string) |
            (?:
              $[ \t]+ [a-zA-Z0-9]+ [ \t]+ .*^
            )
        )]]></string>
    </dict>
